{
  "name": "tergite-dashboard",
  "private": true,
  "version": "2025.06.0",
  "type": "module",
  "scripts": {
    "test-api": "tsx --env-file=.env.test api/server.ts",
    "dev-api": "tsx watch --env-file=.env.development api/server.ts",
    "dev": "start-server-and-test dev-api http://0.0.0.0:8002 vite",
    "dev-app-only": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "test": "vitest ",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test-api-and-app": "start-server-and-test test-api http://0.0.0.0:8002 \"npm run preview -- --host 0.0.0.0 --port 5173\"",
    "start-e2e-server": "npm run build -- --mode test && start-server-and-test test-api-and-app http://127.0.0.1:5173",
    "e2e": "npm run start-e2e-server -- \"cypress run\"",
    "visual-e2e": "npm run start-e2e-server -- \"cypress open\"",
    "cypress-only": "cypress run",
    "visual-cypress-only": "cypress open"
  },
